{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)53540dd5-7b95-4fc6-97ad-2a38d18b6dbf","ParentGuid":"(Guid)2fbba09b-2b13-413f-adb8-36d728ecbad7","Name":"(string)PRG_SQlite","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)(* state machine *)\nIF xSQlite_Enable THEN\n\tCASE iState OF\n\t\t0\t:\t// idle\n\t\t\t\tIF xSQlite_Connect THEN\n\t\t\t\t\tiState := 10;\n\t\t\t\tELSIF xSQlite_Disconnect THEN\n\t\t\t\t\tiState := 80;\n\t\t\t\tELSIF xSQlite_LogData THEN\n\t\t\t\t\tiState := 40;\n\t\t\t\tEND_IF\n\t\t\t\tsState := 'SQllite idle - wait for command';\n\t\t\t\t\n\t\t10\t:\t// open tDB\n\t\t\t\tIF oSQlite.open(sPath := sSQlite_DBpath, sFilename := sSQlite_Filename, xOnSDCard := xSQlite_OnSDcard, xClassicPath := xSQlite_ClassicPath, xBeginTransaction := xSQlite_BeginTransac, xError => xSQlite_Err, sStatus => sSQlite_Status, xDbExists => xSQlite_DBexist) THEN\n\t\t\t\t\tIF \txSQlite_Err THEN\n\t\t\t\t\t\tiState := 99;\n\t\t\t\t\t\tsState := 'SQllite open DB - open error';\n\t\t\t\t\tELSE\n\t\t\t\t\t\tiState := 11;\n\t\t\t\t\t\tsState := 'SQllite open DB - openning DB';\n\t\t\t\t\tEND_IF\n\t\t\t\tEND_IF\n\t\t\t\t\n\t\t11\t:\t// DB is open\n\t\t\t\txSQlite_Connect := FALSE;\n\t\t\t\tiState := 0;\n\t\t\t\tsState := 'SQllite open DB - DB is open';\n\t\t\t\t\n\t\t40\t:\t// log data, prepare statement\n\t\t\t\t;\n\t\t\t\t\n\t\t80\t:\t// close DB\n\t\t\t\tIF oSQlite.close(xCommitTransaction := TRUE, xError := xSQlite_Err, sStatus := sSQlite_Status) THEN\n\t\t\t\t\tiState := 81;\n\t\t\t\t\tsState := 'SQllite close DB - closing DB';\t\t\t\t\t\n\t\t\t\tEND_IF\n\t\t\t\t\n\t\t81\t:\t// DB is close\n\t\t\t\txSQlite_Disconnect := FALSE;\n\t\t\t\tiState := 0;\n\t\t\t\tsState := 'SQllite close DB - DB is closed';\n\t\t\t\t\n\t\t99\t:\t// error handling\n\t\t\t\t;\n\tELSE\n\t\tiState := 0;\n\tEND_CASE\nELSE\n\tiState := 0;\nEND_IF\n\n{region 'magt-state-fuffer'}\n\tIF sState <> aState[0] THEN \n\t\tFOR i:= c_iStateBufferSize TO 1 BY -1 DO\n\t\t\taState[i] := aState[i-1];\n\t\tEND_FOR\n\tEND_IF\n\taState[0] := sState;\n\tIF xSQlite_ClearLog\tTHEN\n\t\tFOR i:= 0 TO c_iStateBufferSize DO\n\t\t\taState[i] := '';\n\t\tEND_FOR\n\t\txSQlite_ClearLog := FALSE;\n\tEND_IF\n{endregion}","LineInfoPersistence":"(string)53540dd5-7b95-4fc6-97ad-2a38d18b6dbf_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)PROGRAM PRG_SQlite\nVAR\n\tiState\t\t\t\t\t\t\t: INT;\n\tsState\t\t\t\t\t\t\t: STRING;\n\taState\t\t\t\t\t\t\t: ARRAY[0..c_iStateBufferSize] OF STRING;\n\txSQlite_Enable\t\t\t\t\t: BOOL := TRUE;\n\txSQlite_Connect\t\t\t\t\t: BOOL;\n\txSQlite_Disconnect\t\t\t\t: BOOL;\n\txSQlite_LogData\t\t\t\t\t: BOOL;\n\txSQlite_ClearLog\t\t\t\t: BOOL;\n\t\n\ti\t\t\t\t\t\t\t\t: INT;\n\t\n\tsSQlite_DBpath\t\t\t\t\t: STRING := '/media/sdcard/sqlite';\n\tsSQlite_Filename\t\t\t\t: STRING := 'db1';\n\txSQlite_OnSDcard\t\t\t\t: BOOL := FALSE;\n\txSQlite_ClassicPath\t\t\t\t: BOOL := FALSE;\n\t\n\t\n\t(* instances *)\n\toSQlite\t\t\t\t\t\t\t: WagoSysSQLite_FR.FbSQLite;\n\txSQlite_BeginTransac \t\t\t: BOOL;\n\txSQlite_Err\t\t\t\t\t\t: BOOL;\n\tsSQlite_Status\t\t\t\t\t: STRING;\n\txSQlite_DBexist\t\t\t\t\t: BOOL;\n\t\n\t\n\t\nEND_VAR\nVAR CONSTANT\n\tc_iStateBufferSize\t\t\t\t: INT := 19;\nEND_VAR","LineInfoPersistence":"(string)53540dd5-7b95-4fc6-97ad-2a38d18b6dbf_Decl_LineIds"}}}}